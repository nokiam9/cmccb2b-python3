# /bin/bash

# export table
mongoexport -d cmccb2b -c Cmccb2bItem -o Cmccb2bItem.json
gzip Cmccb2bItem.json

# import table && add indexes for flask
cat Cmccb2bItem.json.sample.gz| gzip -d |mongoimport -d cmccb2b -c Cmccb2bItem --drop
mongo add_index.js

